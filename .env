# Copy to .env
PORT=3000
REQUIRE_AUTH=false        # set to true to require JWT on /api/files and /api/stream
JWT_SECRET=change_this    # change before using REQUIRE_AUTH=true
TORRENT_TMP_DIR=./tmp     # where WebTorrent will store partial files (must be writable)
TTR_CLEANUP_MINUTES=5     # minutes of inactivity after which torrent is destroyed
